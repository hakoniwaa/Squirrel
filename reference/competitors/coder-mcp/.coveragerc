[run]
source = coder_mcp
branch = True
parallel = True
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */.venv/*
    */site-packages/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    @(abc\.)?abstractmethod
    if TYPE_CHECKING:
    if typing.TYPE_CHECKING:

ignore_errors = True
show_missing = True
precision = 2
skip_covered = False
skip_empty = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
